{
    "redirects": [
        {
            "source": "/.well-known/assetlinks.json",
            "destination": "/public/assetlinks.json",
            "permanent": false,
            "statusCode": 200
        }
    ],
    "headers": [
        {
            "source": "/.well-known/assetlinks.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
                },
                {
                    "key": "Pragma",
                    "value": "no-cache"
                },
                {
                    "key": "Expires",
                    "value": "0"
                },
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ]
}